{
  "name": "fz-ui",
  "version": "0.0.27",
  "description": "Vue3 UI Components",
  "author": {
    "name": "tongren",
    "email": "fangzhioooo@gmail.com"
  },
  "homepage": "https://fangzhioo.github.io/fz-ui",
  "typings": "lib/packages/index.d.ts",
  "main": "lib/fzui.umd.js",
  "module": "lib/fzui.es.js",
  "license": "MIT",
  "scripts": {
    "dev": "vite --config ./build/doc.config.ts",
    "build:doc": "vue-tsc --noEmit && vite build --config ./build/doc.config.ts && node scripts/copyDir.js",
    "build:lib": "vue-tsc --noEmit && vite build --config ./build/lib.config.ts",
    "build:esm": "vue-tsc --noEmit && vite build --config ./build/lib.esm.config.ts",
    "build:umd": "vue-tsc --noEmit && vite build --config ./build/lib.umd.config.ts",
    "gen": "node ./scripts/genNewComp/index.js",
    "gen:f": "node ./scripts/genNewComp/index.js just-gen",
    "p": "npm publish --registry https://registry.npmjs.org",
    "version": "npm version patch && npm run p",
    "version:minor": "npm version minor && npm run p",
    "version:major": "npm version major && npm run p",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore src --fix",
    "format": "prettier . --ignore-path .gitignore --write"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.2",
    "@vueuse/core": "^8.5.0",
    "dayjs": "^1.10.7",
    "lodash-es": "^4.17.21",
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@tiptap/extension-highlight": "^2.0.0-beta.33",
    "@tiptap/extension-image": "^2.0.0-beta.27",
    "@tiptap/extension-placeholder": "^2.0.0-beta.48",
    "@tiptap/extension-table": "^2.0.0-beta.49",
    "@tiptap/extension-table-cell": "^2.0.0-beta.20",
    "@tiptap/extension-table-header": "^2.0.0-beta.22",
    "@tiptap/extension-table-row": "^2.0.0-beta.19",
    "@tiptap/extension-task-item": "^2.0.0-beta.32",
    "@tiptap/extension-task-list": "^2.0.0-beta.26",
    "@tiptap/extension-text-align": "^2.0.0-beta.29",
    "@tiptap/starter-kit": "^2.0.0-beta.185",
    "@tiptap/vue-3": "^2.0.0-beta.91",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.4",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "@vitejs/plugin-vue": "^2.0.0",
    "async-validator": "^4.1.1",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.2.0",
    "fs-extra": "^10.0.0",
    "handlebars": "^4.7.7",
    "inquirer": "^8.2.0",
    "less": "^4.1.2",
    "markdown-it-anchor": "^8.4.1",
    "markdown-it-prism": "^2.2.4",
    "normalize-wheel-es": "^1.1.1",
    "prettier": "^2.5.1",
    "prismjs": "^1.25.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-esbuild": "^4.8.2",
    "rollup-plugin-vue": "^6.0.0",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vite-plugin-banner": "^0.1.3",
    "vite-plugin-dts": "^0.9.9",
    "vite-plugin-md": "^0.11.6",
    "vue-router": "^4.0.12",
    "vue-tsc": "^0.29.8"
  }
}
