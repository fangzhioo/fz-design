{
  "name": "fz-ui",
  "version": "0.0.6",
  "description": "Vue3 UI Components",
  "typings": "lib/packages/index.d.ts",
  "main": "lib/fzui.umd.js",
  "module": "lib/fzui.es.js",
  "license": "MIT",
  "scripts": {
    "dev": "vite --config ./build/base.config.ts",
    "build:doc": "vue-tsc --noEmit && vite build --config ./build/doc.config.ts && node script/copyDir.js",
    "build:lib": "vue-tsc --noEmit && vite build --config ./build/lib.config.ts",
    "gen": "node ./script/genNewComp/index.js",
    "gen:reflesh": "node ./script/genNewComp/index.js just-gen",
    "publish": "npm publish --registry https://registry.npmjs.org",
    "version": "npm version patch && npm run publish",
    "version:minor": "npm version minor && npm run publish",
    "version:major": "npm version major && npm run publish"
  },
  "peerDependencies": {
    "vue": "^3.2.25"
  },
  "dependencies": {
    "@vueuse/core": "^7.4.1",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.4",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "@vitejs/plugin-vue": "^2.0.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.2.0",
    "fs-extra": "^10.0.0",
    "handlebars": "^4.7.7",
    "inquirer": "^8.2.0",
    "less": "^4.1.2",
    "prettier": "^2.5.1",
    "prismjs": "^1.25.0",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vite-plugin-dts": "^0.9.6",
    "vite-plugin-md": "^0.11.6",
    "vue-router": "^4.0.12",
    "vue-tsc": "^0.29.8"
  }
}
